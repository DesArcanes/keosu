{
   "name" : "keosu-push",
   "description" : "This allow to send push message to your users",
   "version" : "0.1",
   "type" : "plugin",
   "require" : [
      { "name" : "angularJs" , "version" : ">=0.1" }
   ],
   "libJs" : [
      "PushNotification.js"
   ],
   "configCordova": [
      { "gap:plugin" : [
         { "@attributes" : {"name":"com.adobe.plugins.pushplugin"} }
      ]}
   ],
   "appParam" : [
      { "name" : "privateApp", "type" : "checkbox" , "options" : {
         "required" : 0
      }}
   ]
}
