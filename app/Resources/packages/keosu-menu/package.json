{
    "name" : "keosu-menu",
    "description" : "Create a menu gadget",
    "version" : "0.1",
    "type" : "gadget",
    "param" : [
        { "pages" : [{
                    "id"   : "integer",
                    "icon" : "text"
                    }]
        }
    ],
    "require" : [
        { "name" : "angularJs" , "version" : ">=1.2" },
        { "name" : "jQuery", "version" : ">=2"}
    ],
    "configCordova" : [
        { "gap:plugin" : [
            { "@attributes" : {"name" : "com.phonegap.plugins.facebookconnect" , "version":"0.4.0"} },
            { "param" : [ {"@attributes" : {"name" : "test","value":"@@test2@@"} } ] },
            { "param" : [ {"@attributes" : {"name" : "test2","value":"@@test1@@"}}]}
    ]}],
    "libJs" : [],
    "appParam" : [
        {"name" : "test" , "type" : "text"},
        {"name" : "test2", "type" : "checkbox", "options" : {
                "required" : 0
        }}
    ]
}
