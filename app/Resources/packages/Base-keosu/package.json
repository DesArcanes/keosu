{
    "name" : "keosu-base",
    "description" : "contains all the required package and template for keosu",
    "version" : "0.1",
    "type" : "base",
	"configCordova" : [
		{ "gap:plugin" :[
			{ "@attributes" : {"name" : "org.apache.cordova.network-information" , "version":"0.2.7" } }
		]},
		{ "gap:plugin" : [
			{ "@attributes" : {"name" : "com.phonegap.plugin.statusbar"} }
		]},
		{ "gap:plugin" : [
			{ "@attributes" : {"name" : "org.apache.cordova.splashscreen",  "version":"0.3.4" } }
		]}
	],
	"pluginToInstall" : [
		"cordova plugin add org.apache.cordova.network-information"
	],
    "require" : [
        { "name" : "angularJs" , "version" : ">=1.2" },
        { "name" : "jQuery", "version" : ">=2" },
        { "name" : "keosu-push" , "version" : ">=0.1"}
    ]
}
